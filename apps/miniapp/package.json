{
  "name": "@app/miniapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "prepare": "node ./bin/prepare.js",
    "dev": "yarn build:deps && yarn dev:start",
    "dev:start": "node --trace-warnings ./../../bin/run-keystone-app.js",
    "debug": "node inspect node_modules/.bin/keystone dev",
    "build:deps": "turbo build --filter=@app/miniapp^... --filter=!@app/condo",
    "build": "cross-env PHASE=build keystone build",
    "start": "NODE_ENV=production node ./../../bin/run-keystone-app.js",
    "worker": "cross-env PHASE=worker node worker.js",
    "kmigrator": "./../../bin/kmigrator.py",
    "makemigrations": "./../../bin/kmigrator.py makemigrations",
    "migrate": "./../../bin/kmigrator.py migrate",
    "migrate:down": "./../../bin/kmigrator.py down",
    "migrate:unlock": "./../../bin/kmigrator.py unlock",
    "maketypes:miniapp": "mkdir -p dist/admin && node ./../../bin/create-graphql-schema.js StitchSchemaMiddleware && graphql-codegen -c codegen.yaml",
    "maketypes:condo": "node ./bin/generate-condo-schema.js && graphql-codegen -c condoCodegen.yaml",
    "maketypes": "yarn maketypes:miniapp && yarn maketypes:condo",
    "createschema": "./../../bin/createschema",
    "createservice": "node ./../../bin/createservice"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@app/condo": "workspace:^",
    "@condo/domains": "link:../condo/domains",
    "@emotion/babel-plugin": "^11.10.0",
    "@emotion/core": "^10.1.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.10.0",
    "@keystonejs/adapter-knex": "^13.2.0",
    "@keystonejs/adapter-mongoose": "^11.2.0",
    "@keystonejs/adapter-prisma": "^3.2.0",
    "@keystonejs/app-admin-ui": "^7.5.0",
    "@keystonejs/app-graphql": "^6.3.0",
    "@keystonejs/app-graphql-playground": "^5.2.2",
    "@keystonejs/app-next": "^5.2.3",
    "@keystonejs/app-static": "^5.2.0",
    "@keystonejs/auth-password": "^6.1.0",
    "@keystonejs/fields": "^22.1.3",
    "@keystonejs/fields-color": "^1.1.0",
    "@keystonejs/fields-content": "^9.1.0",
    "@keystonejs/fields-location-google": "^3.4.0",
    "@keystonejs/fields-markdown": "^5.3.2",
    "@keystonejs/fields-wysiwyg-tinymce": "^5.4.0",
    "@keystonejs/file-adapters": "^7.1.0",
    "@keystonejs/keystone": "^19.3.1",
    "@keystonejs/list-plugins": "^8.1.0",
    "@keystonejs/server-side-graphql-client": "^2.1.0",
    "@keystonejs/utils": "^6.1.0",
    "@miniapp/domains": "link:./domains",
    "@open-condo/bridge": "workspace:^",
    "@open-condo/codegen": "workspace:^",
    "@open-condo/config": "workspace:^",
    "@open-condo/keystone": "workspace:^",
    "@open-condo/next": "workspace:^",
    "@open-condo/ui": "workspace:^",
    "@welldone-software/why-did-you-render": "^5.0.0-alpha.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "ajv": "8.11.0",
    "antd": "^4.24.12",
    "classnames": "2.2.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
    "express": "^4.17.1",
    "facepaint": "^1.2.1",
    "get-contrast": "^2.0.0",
    "graphql-upload": "^15.0.1",
    "immer": "9.0.13",
    "less": "^3.12.2",
    "lodash": "^4.17.21",
    "next": "^9.5.5",
    "next-transpile-modules": "^4.1.0",
    "numeral": "2.0.6",
    "qs": "^6.9.4",
    "rc-queue-anim": "^1.8.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fittext": "^1.0.0",
    "react-markdown": "^4.3.1",
    "react-responsive": "^8.1.0",
    "react-test-renderer": "^16.13.1",
    "tslib": "^2.5.3",
    "use-immer": "^0.4.1",
    "uuid": "^8.3.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^16",
    "@types/react": "16.9.17",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-jest": "^29.7.0",
    "css-loader": "1.0.1",
    "extracted-loader": "^1.0.7",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-jasmine2": "^29.7.0",
    "jest-watch-typeahead": "0.6.5",
    "less-loader": "^4.0.0",
    "pluralize": "^8.0.0",
    "raw-loader": "^4.0.2",
    "typescript": "^5.5.4",
    "webpack": "^4.46.0"
  },
  "repository": "https://github.com/open-condo-software/condo/tree/main/apps/miniapp"
}
