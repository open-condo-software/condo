{
  "name": "@app/helpdesk-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn build:deps && next dev & yarn types:watch",
    "build:deps": "turbo build --filter=@app/helpdesk-web^... --filter=!@app/condo",
    "build": "next build",
    "build_dev": "SERVER_URL=https://demo.dev.doma.ai next build",
    "build_prod": "SERVER_URL=https://v1.doma.ai next build",
    "start": "next start",
    "test": "next build",
    "migrate": "echo 1",
    "worker": "echo 1",
    "maketypes": "graphql-codegen -c codegen.ts",
    "types:watch": "graphql-codegen -c codegen.ts --watch",
    "lint-translations": "DISABLE_NEXT_APP=true node ./../../bin/lint-i18n-translations.js"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@apollo/client": "^3.11.8",
    "@app/condo": "workspace:^",
    "@condo/domains": "link:../condo/domains",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@helpdesk-web/domains": "link:./domains",
    "@open-condo/apollo": "workspace:^",
    "@open-condo/cli": "workspace:^",
    "@open-condo/config": "workspace:^",
    "@open-condo/icons": "workspace:^",
    "@open-condo/locales": "workspace:^",
    "@open-condo/miniapp-utils": "workspace:^",
    "@open-condo/next": "workspace:^",
    "@open-condo/ui": "workspace:^",
    "@types/lodash": "4.14.182",
    "antd": "^4.24.12",
    "classnames": "^2.5.1",
    "cookies-next": "^4.2.1",
    "dayjs": "^1.11.10",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "http-proxy": "^1.18.1",
    "iron-session": "^8.0.4",
    "less": "^4.3.0",
    "less-loader": "^12.3.0",
    "lodash": "^4.17.21",
    "next": "^15.4.6",
    "next-with-less": "^3.0.1",
    "openid-client": "5.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^8.3.0"
  },
  "repository": "https://github.com/open-condo-software/condo_call_center/",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^22",
    "@types/react": "^18.3.1",
    "typescript": "^5.8.3"
  }
}
