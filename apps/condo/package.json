{
  "name": "@app/condo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "mkdirp dist/admin && cross-env NODE_OPTIONS=--experimental-vm-modules jest --unhandled-rejections=strict",
    "prepare": "node ./bin/prepare.js",
    "dev": "yarn build:deps && cross-env NODE_OPTIONS=\"--max_old_space_size=6144\" NODE_ENV=development node --trace-warnings ./../../bin/run-keystone-app.js & yarn types:watch",
    "debug": "node inspect node_modules/.bin/keystone dev",
    "build:static": "yarn workspace @open-condo/ui build:sb -o ../../apps/condo/public/ui && mv -f public/ui/ui/* public/ui",
    "build:deps": "turbo build --filter=@app/condo^...",
    "build": "yarn build:static && cross-env NODE_OPTIONS=\"--max_old_space_size=6144\" PHASE=build keystone build",
    "start": "NODE_ENV=production node ./../../bin/run-keystone-app.js",
    "worker": "cross-env PHASE=worker DISABLE_NEXT_APP=true node worker.js",
    "kmigrator": "./../../bin/kmigrator.py",
    "makemigrations": "./../../bin/kmigrator.py makemigrations",
    "migrate": "./../../bin/kmigrator.py migrate",
    "migrate:down": "./../../bin/kmigrator.py down",
    "migrate:unlock": "./../../bin/kmigrator.py unlock",
    "maketypes": "mkdirp dist/admin && DEFAULT_LOCALE=en node ./../../bin/create-graphql-schema.js && graphql-codegen -c codegen.ts",
    "types:watch": "graphql-codegen -c codegen.ts --watch",
    "createservice": "node ./../../bin/createservice",
    "createschema": "node ./../../bin/createschema",
    "lint-schema": "DISABLE_NEXT_APP=true node ./../../bin/lint-schema.js",
    "lint-translations": "DISABLE_NEXT_APP=true node ./../../bin/lint-i18n-translations.js"
  },
