    "types:watch": "yarn maketypes --watch"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.5.6",
    "@apollo/client": "^3.8.4",
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@dev-portal/domains": "link:./domains",
    "@open-condo/cli": "workspace:^",
    "@open-condo/config": "workspace:^",
    "@open-condo/icons": "workspace:^",
    "@open-condo/next": "workspace:^",
    "@open-condo/tsconfig": "workspace:^",
    "@open-condo/ui": "workspace:^",
    "antd": "5.1.7",
    "cookies-next": "2.1.1",
    "deepmerge": "^4.3.1",
    "glob": "8.1.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "gray-matter": "4.0.3",
    "lodash": "^4.17.21",
    "next": "13.3.0",
    "next-mdx-remote": "4.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.2.7",
    "rehype-prism-plus": "1.5.1",
    "remark-custom-heading-id": "1.0.0",
    "remark-gfm": "3.0.1",
    "title": "3.5.3",
