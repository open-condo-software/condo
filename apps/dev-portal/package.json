{
  "name": "@app/dev-portal",
  "version": "0.0.0-development",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "build:deps": "turbo build --filter=dev-portal^...",
    "dev": "yarn build:deps && next dev & yarn types:watch",
