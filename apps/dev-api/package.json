{
  "name": "@app/dev-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest --unhandled-rejections=strict",
    "prepare": "node bin/prepare.js",
    "build:deps": "turbo build --filter=dev-api^...",
    "build": "cross-env PHASE=build keystone build",
    "dev": "node --trace-warnings ./../../bin/keystone-dev.js",
    "start": "keystone start",
    "makemigrations": "./../../bin/kmigrator.py makemigrations",
    "migrate": "./../../bin/kmigrator.py migrate",
    "migrate:down": "./../../bin/kmigrator.py down",
    "maketypes": "mkdir -p dist/admin && NODE_ENV=test node ./../../bin/create-graphql-schema.js --no-codegen",
    "createschema": "./../../bin/createschema",
    "createservice": "node ./../../bin/createservice"
  },
  "dependencies": {
    "@dev-api/domains": "link:./domains",
    "@faker-js/faker": "^7.6.0",
    "@keystonejs/adapter-knex": "^13.2.0",
    "@keystonejs/adapter-mongoose": "^11.2.0",
    "@keystonejs/adapter-prisma": "^3.2.0",
    "@keystonejs/app-admin-ui": "^7.5.0",
    "@keystonejs/app-graphql": "^6.3.0",
    "@keystonejs/app-graphql-playground": "^5.2.2",
    "@keystonejs/auth-password": "^6.1.0",
    "@keystonejs/fields": "^22.1.3",
    "@keystonejs/fields-color": "^1.1.0",
    "@keystonejs/fields-content": "^9.1.0",
    "@keystonejs/fields-markdown": "^5.3.2",
    "@keystonejs/fields-wysiwyg-tinymce": "^5.4.0",
    "@keystonejs/file-adapters": "^7.1.0",
    "@keystonejs/keystone": "^19.3.1",
    "@keystonejs/list-plugins": "^8.1.0",
    "@keystonejs/server-side-graphql-client": "^2.1.0",
    "@keystonejs/utils": "^6.1.0",
    "@open-condo/apollo-server-client": "workspace:^",
    "@open-condo/codegen": "workspace:^",
    "@open-condo/config": "workspace:^",
    "@open-condo/keystone": "workspace:^",
    "@open-condo/locales": "workspace:^",
    "dayjs": "^1.11.9",
    "express": "^4.17.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "phone": "^3.1.40",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^16",
    "babel-jest": "^29.4.0",
    "cross-env": "^7.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.0",
    "jest-jasmine2": "^29.4.0",
    "jest-watch-typeahead": "^0.6.1"
  },
  "repository": "https://github.com/open-condo-software/condo/tree/master/apps/dev-api"
}
