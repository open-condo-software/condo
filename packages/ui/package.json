    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@linaria/babel-preset": "^4.1.2",
    "@linaria/core": "^4.1.1",
    "@linaria/react": "^4.1.2",
    "@linaria/webpack-loader": "^4.1.2",
    "@types/react": "^18.0.17",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
