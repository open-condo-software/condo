{
  "name": "@condo/ui",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.0.1",
  "description": "A set of React UI components for developing applications inside the condo ecosystem",
  "repository": "https://github.com/open-condo-software/condo/blob/master/packages/ui",
  "author": {
    "name": "Matthew Savelev",
    "email": "msavelyev@doma.ai",
    "url": "https://github.com/SavelevMatthew"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:babel": "webpack",
    "build:sb": "build-storybook",
    "build:types": "tsc --emitDeclarationOnly --declarationMap",
    "build": "yarn build:babel && yarn build:types",
    "dev": "start-storybook -p 6006"
