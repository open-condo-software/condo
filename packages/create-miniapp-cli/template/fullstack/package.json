{
  "name": "@app/billing-connector",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "node ./bin/prepare.js",
    "test": "jest",
    "dev": "yarn build:deps && NODE_ENV=development node --trace-warnings ./../../bin/run-keystone-app.js & yarn types:watch",
    "worker": "cross-env PHASE=worker node worker.js",
    "debug": "node inspect node_modules/.bin/keystone dev",
    "build:deps": "turbo build --filter=@app/billing-connector^... --filter=!@app/condo",
    "build": "keystone build",
    "start": "NODE_ENV=production node ./../../bin/run-keystone-app.js",
    "kmigrator": "./../../bin/kmigrator.py",
    "makemigrations": "./../../bin/kmigrator.py makemigrations",
    "migrate": "./../../bin/kmigrator.py migrate",
    "migrate:down": "./../../bin/kmigrator.py down",
    "maketypes": "mkdir -p dist/admin && node ./../../bin/create-graphql-schema.js StitchSchemaMiddleware && ./../../node_modules/.bin/graphql-codegen -c codegen.yaml && graphql-codegen -c codegen.ts",
    "createschema": "node ./../../bin/createschema",
    "createservice": "node ./../../bin/createservice",
    "generate-condo-schema": "node ./bin/generate-condo-schema && ./../../node_modules/.bin/graphql-codegen -c condoCodegen.yaml",
    "add-admin": "node ./bin/addAdmin.js",
    "types:watch": "graphql-codegen -c codegen.ts --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@billing-connector/domains": "link:./domains",
    "@keystonejs/app-next": "^5.2.3",
    "@open-condo/apollo-server-client": "workspace:^",
    "@open-condo/bridge": "workspace:^",
    "@open-condo/cli": "workspace:^",
    "@open-condo/codegen": "workspace:^",
    "@open-condo/config": "workspace:^",
    "@open-condo/keystone": "workspace:^",
    "@open-condo/locales": "workspace:^",
    "@open-condo/next": "workspace:^",
    "@open-condo/ui": "workspace:^",
    "@pdf-lib/fontkit": "^1.1.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.24.12",
    "body-parser": "^1.19.1",
    "body-parser-xml": "^2.0.3",
    "dayjs": "^1.10.6",
    "express": "^4.17.1",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "jszip": "^3.10.1",
    "less": "^3.12.2",
    "lodash": "^4.17.21",
    "next": "^9.5.2",
    "next-transpile-modules": "^4.1.0",
    "openid-client": "4.7.4",
    "pdf-lib": "^1.17.1",
    "phone": "3.1.36",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@graphql-tools/graphql-file-loader": "^7.3.11",
    "@graphql-tools/load": "^7.5.10",
    "@types/node": "^16",
    "@types/react": "16.9.17",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-jasmine2": "^29.7.0",
    "pluralize": "^8.0.0",
    "raw-loader": "^4.0.2",
    "ts-jest": "^29.3.1",
    "typescript": "^5.5.4",
    "webpack": "^4.46.0"
  },
  "resolutions": {
    "@types/react": "16.9.17",
    "graphql": "16.10.0"
  }
}
