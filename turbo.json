{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**"]
    },
    "lint-translations":{
      "cache": false,
      "outputs": []
    },
    "prepare": {
      "dependsOn": ["^prepare"],
      "cache": false
    }
  }
}