{
  "name": "root",
  "description": "Easy and Fast StartUp development kit",
  "private": true,
  "repository": "https://github.com/open-condo-software/condo",
  "homepage": "https://github.com/open-condo-software/condo",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx",
    "createapp": "node ./bin/createapp",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@graphql-codegen/cli": "1.21.8",
    "@graphql-codegen/typescript": "1.23.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-eslint": "^10.1.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "7.0.4",
    "jest": "^26.6.3",
    "jest-diff": "^26.0.0",
    "multi-semantic-release": "^3.0.1",
    "postcss-less": "^6.0.0",
    "semantic-release": "^19.0.5",
    "stylelint": "^14.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "turbo": "^1.7.0",
    "typescript": "^4.1.5"
  },
  "engines": {
    "node": ">=14.19",
    "yarn": ">=3.2.0"
  },
  "dependencies": {
    "bull": "^4.8.4",
    "bull-repl": "^0.27.2",
    "commitlint-plugin-function-rules": "^1.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "packageManager": "yarn@3.2.2",
  "resolutions": {
    "@types/react": "16.9.17",
    "npm": "latest-6"
  }
}
